  "bookmarks": [
    {
      "title": "FrogFind!",
      "url": "http://frogfind.com",
      "description": "Suchmaschine und Webseiten-Textvereinfacher für alte Browser."
    },
    {
      "title": "Textise",
      "url": "https://www.textise.net/",
      "description": "Wandelt moderne Websites in reinen Text um."
    },
    {
      "title": "DuckDuckGo Lite",
      "url": "https://duckduckgo.com/lite",
      "description": "Datenschutzfreundliche Suchmaschine ohne Tracking."
    },
    {
      "title": "Wiby",
      "url": "https://wiby.me/",
      "description": "Suchmaschine speziell für kleine, alte und persönliche Webseiten."
    },
    {
      "title": "Marginalia Search",
      "url": "https://search.marginalia.nu/",
      "description": "Fokussiert auf kleine, nicht SEO-optimierte Websites."
    },
    {
