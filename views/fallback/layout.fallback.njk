<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
    <title>
      {% block title %}Mini-App{% endblock %}
    </title>
    {% set styles = style | split(',') %}
    {% for s in styles %}
      <link rel="stylesheet" href="/public/css/{{ s }}.min.css">
    {% endfor %}
  </head>
